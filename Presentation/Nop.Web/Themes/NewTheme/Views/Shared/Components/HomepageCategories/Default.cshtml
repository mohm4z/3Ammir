    <div class="category-grid home-page-category-grid" style="margin-bottom:180px;">
        <div class="MyCotggamptron">
            <div class="container MyHomeContainer">
                <div class="item-grid MyCatggr">
                    <div class="row">
                        <h1 class="pull-left" style="color:#fff">Our Services</h1>
                    </div>

                    <div class="row">
                        <div class="col-xs-8">
                            <hr class="Myhr pull-left" style="width:80px; border: 2px solid #ffd800;" />
