                    <div class="row MyCatgrow">
                        @foreach (var item in Model)
                        {
                            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 MyCustCol">
                                <a class="MyCatga" href="@Url.RouteUrl("Category", new { SeName = item.SeName })" title="@item.PictureModel.Title">
                                    <div class="Mydiv">
                                        <div class="background" style="background-image:url(@item.PictureModel.ImageUrl);">
                                            <div class="Mydiv2">
                                                <div class="transbox">
                                                    <p>@item.Name</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
}


